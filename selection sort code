#include<stdio.h>
void printarray(int *A,int n)
{
     int i;
     for(i=0;i<n;i++)
     {
             printf(" %d",A[i]);
             }
             printf("\n");
             }
void selectionsort(int *A,int n)
{ 
     int temp;
     int i;
     int j;
     int indexofmin=i;
     for( i=0;i<n-1;i++)
     {
             for( j=i+1;j<n-1;j++)
             {
                     if(A[j]<A[indexofmin]){
                     indexofmin=j;
                     }
             }
                     temp=A[i];
                     A[i]=A[indexofmin];
                     A[indexofmin]=temp;
                     }
                     }
                     
                     
int main()
{
    int A[]={12,34,2,43,28,63};
    int n=6;
    printarray(A,n);
    selectionsort(A,n);
    printarray(A,n);
    #include<stdio.h>
void printarray(int *A,int n)
{
     int i;
     for(i=0;i<n;i++)
     {
             printf("%d",A[i]);
             }
             printf("\n");
             }
void bubblesort(int *A,int n)
{ 
     int temp;
     int i;
     int j;
     for( i=0;i<n-1;i++)
     {
             for( j=0;j<n-i-1;j++)
             {
                     if(A[j]>A[j+1])
             {
                     temp=A[j];
                     A[j]=A[j+1];
                     A[j+1]=temp;
                     }
                     }
                     }
                     }
int main()
{
    int A[]={12,34,2,43,28,63};
    int n=6;
    printarray(A,n);
    selectionsort(A,n);
    printarray(A,n);
    return 0;
}

}
